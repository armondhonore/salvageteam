		<panel id="mapLoadingPanel" childLayout="vertical">
			<panel height="40%" />
			<panel childLayout="vertical" align="center" valign="bottom" height="5%" width="40%">	
				<control name="loadingmapbar" width="100%" height="100%" />
			</panel>
		</panel>	
	</layer>
